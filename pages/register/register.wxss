.container{
  display: flex;
  flex-direction: column;
  padding: 50rpx;
  justify-content: center;
  font-size: 30rpx;
}
.text{
  display: flex;
  justify-content: center;
  font-size: 40rpx;
}
.wrap{
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: space-around;
  margin: 20rpx 0;
  border-bottom: 1rpx solid #336699;
  height: 80rpx;
}
.wrap input{
  margin-left: 0rpx;
  height: 80rpx;
  
}
.wrap text{
  display: flex;
  justify-content: center;
  line-height: 80rpx;
  width: 20%;
}
.container button{
  width: 100%;
  margin: 20rpx 0;
  background-color: white;
}
